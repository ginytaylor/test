{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}<section data-section-id="search" data-section-type="search">
  {%- if search.performed == false or search.results_count == 0 -%}
    <div class="EmptyState">
      <div class="Container">
        <h1 class="EmptyState__Title Heading u-h5">{% comment %}ly_i18n_replace_for_[ 'search.general.title' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'search.general.title' | t }}{% endcapture %}{% include 'ly-static-string' with '16347038' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</h1>

        {%- if search.performed -%}
          {%- assign filtered_terms = search.terms | replace: '*', '' -%}
          <p>{% comment %}ly_i18n_replace_for_[ 'search.general.no_results_with_terms' | t: terms: filtered_terms ]_begin{% endcomment %}{% capture ly_template %}{{ 'search.general.no_results_with_terms' | t: terms: filtered_terms }}{% endcapture %}{% include 'ly-static-string' with '16347040' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% assign replacement = filtered_terms  %}{% assign ly_template = ly_template | replace: '[[terms]]', replacement %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</p>
        {%- else -%}
          <p>{% comment %}ly_i18n_replace_for_[ 'search.general.content' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'search.general.content' | t }}{% endcapture %}{% include 'ly-static-string' with '16347041' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</p>
        {%- endif -%}

        <div class="EmptyState__Action" >
          <form method="GET" action="/search" class="Form">
            <input class="Form__Input" type="text" name="q" autocomplete="off" autocorrect="off" aria-label="{% comment %}ly_i18n_replace_for_[ 'search.general.input_placeholder' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'search.general.input_placeholder' | t }}{% endcapture %}{% include 'ly-static-string' with '16347042' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}" placeholder="{% comment %}ly_i18n_replace_for_[ 'search.general.input_placeholder' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'search.general.input_placeholder' | t }}{% endcapture %}{% include 'ly-static-string' with '16347042' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}">
            <input type="hidden" name="type" value="product">
          </form>
        </div>
      </div>
    </div>
  {%- else -%}
    {%- paginate search.results by 36 -%}
      <header class="PageHeader">
        <div class="Container">
          <div class="SectionHeader SectionHeader--center">
            <h1 class="SectionHeader__Heading Heading u-h1">{% comment %}ly_i18n_replace_for_[ 'search.general.title' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'search.general.title' | t }}{% endcapture %}{% include 'ly-static-string' with '16347038' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</h1>

            <div class="SectionHeader__Description">
              {%- assign filtered_terms = search.terms | replace: '*', '' -%}
              {% comment %}ly_i18n_replace_for_[ 'search.general.results_with_terms_count' | t: count: search.results_count, terms: filtered_terms ]_begin{% endcomment %}{% if search.results_count == 0 %}{% capture ly_template %}{{ 'search.general.results_with_terms_count' | t: count: search.results_count, terms: filtered_terms }}{% endcapture %}{% include 'ly-static-string' with '16347043' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% assign replacement = search.results_count %}{% assign ly_template = ly_template | replace: '[[count]]', replacement %}{% assign replacement = filtered_terms  %}{% assign ly_template = ly_template | replace: '[[terms]]', replacement %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% elsif search.results_count == 1 %}{% capture ly_template %}{{ 'search.general.results_with_terms_count' | t: count: search.results_count, terms: filtered_terms }}{% endcapture %}{% include 'ly-static-string' with '16347044' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% assign replacement = search.results_count %}{% assign ly_template = ly_template | replace: '[[count]]', replacement %}{% assign replacement = filtered_terms  %}{% assign ly_template = ly_template | replace: '[[terms]]', replacement %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% elsif search.results_count == 2 %}{% capture ly_template %}{{ 'search.general.results_with_terms_count' | t: count: search.results_count, terms: filtered_terms }}{% endcapture %}{% include 'ly-static-string' with '16347045' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% assign replacement = search.results_count %}{% assign ly_template = ly_template | replace: '[[count]]', replacement %}{% assign replacement = filtered_terms  %}{% assign ly_template = ly_template | replace: '[[terms]]', replacement %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% else %}{% capture ly_template %}{{ 'search.general.results_with_terms_count' | t: count: search.results_count, terms: filtered_terms }}{% endcapture %}{% include 'ly-static-string' with '16347045' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% assign replacement = search.results_count %}{% assign ly_template = ly_template | replace: '[[count]]', replacement %}{% assign replacement = filtered_terms  %}{% assign ly_template = ly_template | replace: '[[terms]]', replacement %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}
            </div>
          </div>
        </div>
      </header>

      {%- if search.types contains 'product' -%}
        <div class="ProductList ProductList--grid Grid" data-mobile-count="2" data-desktop-count="4">
          {%- for result in search.results -%}
            {%- assign tablet_item_numerator = 1 -%}
            {%- assign desktop_item_numerator = 1 -%}

            {%- if result.tags contains 'wide' -%}
              {% assign tablet_item_numerator = 2 %}
              {% assign desktop_item_numerator = 2 %}
            {%- endif -%}

            <div class="Grid__Cell 1/2 {{ tablet_item_numerator }}/3--tablet {{ desktop_item_numerator }}/4--lap-and-up">
              {%- include 'product-item', product: result, show_labels: true, show_product_info: true, show_vendor: false -%}
            </div>
          {%- endfor -%}
        </div>
      {%- else -%}
        <div class="ArticleListWrapper">
          <div class="ArticleList Grid Grid--m">
            {%- for result in search.results -%}
              <div class="Grid__Cell 1/2--tablet 1/3--lap-and-up">
                {%- if result.object_type == 'article' -%}
                  {%- include 'article-item', article: result -%}
                {%- elsif result.object_type == 'page' -%}
                  <article class="ArticleItem">
                    <div class="ArticleItem__Content">
                      <h2 class="ArticleItem__Title Heading u-h2">
                        <a href="{{ result.url }}">{% comment %}ly_code_replace_for_[ result.title ]_begin{% endcomment %}{% include 'ly-title' with result %}{{ ly_translation }}{% comment %}ly_code_replace_end{% endcomment %}</a>
                      </h2>

                      <p class="ArticleItem__Excerpt">{% comment %}ly_code_replace_for_[ result.content | strip_html | truncate: 150 ]_begin{% endcomment %}{% include 'ly-content' with result %}{{ ly_translation | strip_html | truncate: 150 }}{% comment %}ly_code_replace_end{% endcomment %}</p>
                      <a href="{{ article.url }}" class="ArticleItem__Link Link Link--underline">{% comment %}ly_i18n_replace_for_[ 'blog.article.read_more' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'blog.article.read_more' | t }}{% endcapture %}{% include 'ly-static-string' with '16347049' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</a>
                    </div>
                  </article>
                {%- endif -%}
              </div>
            {%- endfor -%}
          </div>
        </div>
      {%- endif -%}

      {%- include 'pagination' -%}
    {%- endpaginate -%}
  {%- endif -%}
</section>