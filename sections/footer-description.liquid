{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}<section class="Footer_description_container" id="section-{{ section.id }}">
 
        <div class="Footer-description">
            <header class="Description-title">
              <img class="delivery_Icon" src="{% comment %}ly_asset_replace_for_[ 'shipping-fast-solid.svg' | asset_url ]_begin{% endcomment %}{% assign ly_asset = 'shipping-fast-solid.svg' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  }}{% comment %}ly_asset_replace_end{% endcomment %}">
                <h4 class="SectionHeader__Heading Heading u-h4">{{ section.settings.title-1 | escape }}</h4>
            </header>

            <div class="SectionHeader__Description">
              {{ section.settings.content-1 }}
            </div>
        </div>
        <div class="Footer-description">
            <header class="Description-title">
            <img class="couch_Icon" src="{% comment %}ly_asset_replace_for_[ 'small_sofa.svg' | asset_url ]_begin{% endcomment %}{% assign ly_asset = 'small_sofa.svg' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  }}{% comment %}ly_asset_replace_end{% endcomment %}">
                <h4 class="SectionHeader__Heading Heading u-h4">{{ section.settings.title-2 | escape }}</h4>
            </header>

            <div class="SectionHeader__Description">
              {{ section.settings.content-2 }}
            </div>
        </div>
        <div class="Footer-description">
            <header class="Description-title">
                <img class="care_Icon" src="{% comment %}ly_asset_replace_for_[ 'envelope-regular.svg' | asset_url ]_begin{% endcomment %}{% assign ly_asset = 'envelope-regular.svg' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  }}{% comment %}ly_asset_replace_end{% endcomment %} ">
                <h4 class="SectionHeader__Heading Heading u-h4">{{ section.settings.title-3 | escape }}</h4>
            </header>

            <div class="SectionHeader__Description">
              {{ section.settings.content-3 }}
            </div>
        </div>
        <div class="Footer-description">
            <header class="Description-title">
            <img class="network_Icon" src="{% comment %}ly_asset_replace_for_[ 'network.svg' | asset_url ]_begin{% endcomment %}{% assign ly_asset = 'network.svg' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  }}{% comment %}ly_asset_replace_end{% endcomment %}">
                <h4 class="SectionHeader__Heading Heading u-h4">{{ section.settings.title-4 | escape }}</h4>
            </header>

            <div class="SectionHeader__Description">
              {{ section.settings.content-4 }}
            </div>
        </div>
    
</section>


