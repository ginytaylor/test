<div class="info-left">
	{% assign fields =  product.metafields.saleslayer %}
	{% assign show_tabsu = false %}
	{% assign show_tabdes =  false %}
	{% assign show_tabfa = false %}
	<!-- Tab links -->
	
	{% capture data_content %}
	<!-- Tab content -->

	<div class="tabs-content">
		{% if fields.panet_score or fields.people_score or fields.transparency_score or fields.circularity_store %}
			{% assign show_tabsu = true %}
			<div class="tabs-title hide first-tab">
				<button class="tablinks p-xsmall" data-tab="sustainability">sustainability<span class="icon-plus"></span></button>
			</div>
			<div id="sustainability" class="tabcontent" style="display: block;">
				<!-- circularity at our core -->
				{% if fields.circularity_store %}
				  	<div class="tab-v active">
				  		<div class="tab-title">
				  			<span class="score">
				  				<svg viewBox="0 0 36 36" class="circular">
							      	<path class="circle-bg"
							        d="M18 2.0845
							          a 15.9155 15.9155 0 0 1 0 31.831
							          a 15.9155 15.9155 0 0 1 0 -31.831"
							      />
							      <path class="circle"
							        stroke-dasharray="{{ fields.circularity_store | times: 10 }}, 100"
							        d="M18 2.0845
							          a 15.9155 15.9155 0 0 1 0 31.831
							          a 15.9155 15.9155 0 0 1 0 -31.831"
							      />
							    </svg>
							    <span class="percentage">{{ fields.circularity_store | plus: 0 | divided_by: 1.0 }}</span>
		    				</span>
				  			<span class="title h6">
				  				circularity
				  				<span class="p-xsmall"><em class="cl-red">i am</em> made from previous life fibres</span>
				  			</span>
				  			<span class="icon-plus"></span>
				  		</div>
				  		<div class="tab-v-content">
				  			{% if fields.circularity_percentage  %} 
					  			<div class="item">
					  				<span class="icon-Sustainability_icons_outlined_eco-fabric"></span>
					  				<span>
						  				<strong> {{ fields.circularity_percentage}}% </strong>of my fabrics come from previous life*
					  				</span>
					  				
					  			</div>
				  			{% endif %}
				  		</div>
				  	</div>
			 	{% endif %}
			  	<!-- PLANET TAB -->
			  	{% if fields.panet_score %}
