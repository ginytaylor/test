					  			<div class="item">
					  				
					  				<span class="hide-small">
					  				{% capture h2o %}
					  					<span class="hide-hover" >
					  						<span class="icon-Sustainability_icons_outlined_H2O"></span>
							  				{% if fields.h2o_litre %}
							  					<strong>{{ fields.h2o_litre }}L </strong> <span> of water</span> 
							  				{% endif %}
							  				{% if fields.h2o_saved %}
							  					,<strong> {{ fields.h2o_saved }}% </strong>preserved
							  				{% endif %}
							  			</span>
						  				<span class="show-hover">

						  					{% if fields.water_glasses %}
						  					<span class="icon-Sustainability_icons_outlined_water-glass"></span>
							  				<strong>= {{ fields.water_glasses }} <span>glasses</span></strong> of water preserved 
							  				{% else %}
							  					<span class="icon-Sustainability_icons_outlined_H2O"></span>
								  				{% if fields.h2o_litre %}
								  					<strong>{{ fields.h2o_litre }}L </strong> <span> of water</span>
								  				{% endif %}
								  				{% if fields.h2o_saved %}
								  					,<strong> {{ fields.h2o_saved }}% </strong> preserved
								  				{% endif %}
							  				{% endif %}
							  			</span>
					  				{% endcapture %}
					  				{{ h2o }}
					  				</span>
					  				<span class="show-small">
					  					<span>
					  						<span class="icon-Sustainability_icons_outlined_H2O"></span>
					  						<span>
					  							{% if fields.h2o_litre %}
							  					<strong>{{ fields.h2o_litre }}</strong>L <span>of water</span> 
								  				{% endif %}
								  				{% if fields.h2o_saved %}
								  					,<strong> {{ fields.h2o_saved }}% </strong> preserved
								  				{% endif %} <br/>
								  				{% if fields.water_glasses %}
								  				<strong>= {{ fields.water_glasses }}</strong> glasses of water preserved
								  				{% endif %}
					  						</span>
					  					</span>
					  				</span>
					  			</div>
				  			{% endif %}

				  			{% if fields.chem_gram or fields.chem_saved or field.chem_tomato  %} 
				  				<div class="item">
					  				
					  				<span class="hide-small">
					  				{% capture chemical %}
					  					<span class="hide-hover" >
					  						<span class="icon-Sustainability_icons_outlined_chemical-3"></span>
							  				{% if fields.chem_gram %}
							  					<strong>{{ fields.chem_gram }}g </strong>PO4 eq  
							  				{% endif %}
							  				{% if fields.chem_saved %}
							  					,<strong> {{ fields.chem_saved }}% </strong>preserved
							  				{% endif %}
							  			</span>
							  			<span class="show-hover">
							  				<span class="icon-Sustainability_icons_outlined_tomato"></span>
							  				{% if fields.chem_tomato %}
								  				= <span>nutrients for </span> <strong class="tomatoes">{{fields.chem_tomato}} <span>tomatoes</span> </strong>
							  				{% endif %} preserved
							  			</span>
						  				
					  				{% endcapture %}
					  				{{ chemical  }}
					  				</span>
					  				<span class="show-small">
					  					<span>
					  						<span class="icon-Sustainability_icons_outlined_chemical-3"></span>
					  						<span>
					  							{% if fields.chem_gram %}
								  					<strong>{{ fields.chem_gram }}</strong>g PO4 eq  
								  				{% endif %}
