								    {% if section.settings.title3 != blank %}
								    	{{ section.settings.title3 }}
								    {% else %}
								    	recycled Spandex
								    {% endif %}
								    {% if section.settings.description3 != blank %}
								    	<span class="p-xsmall">{{section.settings.description3}}</span>
