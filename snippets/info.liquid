			  			{% assign fab = fields.fabric_set1 | plus: 0 %}
			  			<div class="item">
			  				{% case fab %}
							  {% when 1 %}
							    {% if section.settings.icon1 != blank %}
							    	<img src="{{ section.settings.icon1 | img_url:'50x50' }} " alt="" />
							    {% else %}
							    	<span class="icon-recycled"></span>
							    {% endif %}
							    <span class="h8">
								    {% if section.settings.title1 != blank %}
								    	{{ section.settings.title1 }}
								    {% else %}
								    	recycled Nylon
								    {% endif %}
