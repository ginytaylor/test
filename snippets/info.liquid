				  		<div class="tab-title">
				  			<span class="score">
				  				<svg viewBox="0 0 36 36" class="circular">
							      	<path class="circle-bg"
							        d="M18 2.0845
							          a 15.9155 15.9155 0 0 1 0 31.831
							          a 15.9155 15.9155 0 0 1 0 -31.831"
							      />
							      <path class="circle"
							        stroke-dasharray="{{ fields.panet_score | times: 10 }}, 100"
							        d="M18 2.0845
							          a 15.9155 15.9155 0 0 1 0 31.831
							          a 15.9155 15.9155 0 0 1 0 -31.831"
							      />
							    </svg>
							    <span class="percentage">{{ fields.panet_score | plus: 0 | divided_by: 1.0 }}</span>
		    				</span>
				  			<span class="title h6">
				  				planet in mind
				  				<span class="p-xsmall"><em class="cl-red">i am</em> made with 100% Ã©co-fabrics</span>
				  			</span>
				  			<span class="icon-plus"></span>
				  		</div>
				  		<div class="tab-v-content">
				  			{% if fields.co2_km or fields.co2_kg or field.co2_saved  %} 
					  			<div class="item">
					  				
					  				<span class="hide-small">

					  				{% capture co2 %}
						  				<span class="hide-hover" >
						  					<span class="icon-Sustainability_icons_outlined_CO2"></span>
							  				{% if fields.co2_kg %}
							  					<strong> {{ fields.co2_kg }}kg </strong>CO2 eq
							  				{% endif %}
							  				{% if fields.co2_saved %}
							  					,<strong> {{ fields.co2_saved }}% </strong>preserved
							  				{% endif %}
							  			</span>
							  			<span class="show-hover">
						  				{% if fields.co2_km %}
						  					<span class="icon-Sustainability_icons_outlined_lightbulb"></span>
							  				<strong> = {{ fields.co2_km }} <span> hours of light bulbs on (LED) preserved</span> </strong>  
						  				{% endif %}
						  				</span>
					  				{% endcapture %}
					  				{{ co2 }}
					  				</span>
					  				<span class="show-small">
						  				<span>
						  					<span class="icon-Sustainability_icons_outlined_CO2"></span>
						  					<span>
								  				{% if fields.co2_kg %}
								  					<strong> {{ fields.co2_kg }}</strong>kg CO2 e
								  				{% endif %}
								  				{% if fields.co2_saved %}
								  					,<strong> {{ fields.co2_saved }}% </strong>preserved
								  				{% endif %}
								  				<br />
								  				{% if fields.co2_km %}
									  				<strong> = {{ fields.co2_km }}</strong><span> hours of light bulbs on (LED) preserved</span>  
								  				{% endif %}  
								  			</span>
						  				</span>
					  				</span>
					  			</div>
				  			{% endif %}
